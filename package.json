  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "npm-run-all": "^4.1.5",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "@types/arcgis-js-api": "^4.14.0"
  }
}
